super + Return
    $TERMINAL

super + i
    fol Tmux "sh -c 'tmux a || tmux'" 1

super + shift + i
    pgrep emacs && xdo activate -N Emacs || emacs

super + b
    fol $BROWSER $BROWSER 0

super + shift + w
    weather-report show

super + {d, shift + d, p, shift + p}
    {dmenu_run, dmenuunicode -i, passmenu, displayselect}

super + { m, shift + m}
    {dmenumount, dmenuumount}

super + {Insert, shift + Insert, shift + b}
    {dmenu-bookmarks, bookmarkit, dmenusnippet}

super + v
    clipmenu

super + shift + v
    fol Pulsemixer pulsemixer 1

super + {BackSpace, shift + BackSpace}
    {fol Sdcv sdcv 1 , fol Transs transs 1}

super + {slash, shift + slash}
    {transs p, transs c}

super + e
    fol Lfub lfub 1

super + shift + e
    fol Neomutt "bicon -e neomutt" 1

super + n
    fol Newsboat "bicon -e newsboat" 1

super + shift + n
    fol Ncmpcpp "bicon -e ncmpcpp" 1

Print
    maim "$XDG_PICTURES_DIR/screenshots/$(date '+%y%m%d-%H%M-%S').png" pic-full- && notify-send -t 500 'Screenshot has been printed'

{shift + Print, super + Print, super + Delete}
    {maimpick, dmenurecord -i, dmenurecord kill}

XF86Audio{RaiseVolume,LowerVolume}
    wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%{-,+}

{XF86AudioMute, super+shift+minus}
    wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle

super + { minus, equal}
    wpctl set-volume @DEFAULT_AUDIO_SINK@ {5%-, 5%+}

{XF86MonBrightnessDown, super + XF86MonBrightnessDown, XF86MonBrightnessUp, super + XF86MonBrightnessUp}
	brightnessctl set {10%-, 1%, +10%, 100%}

super + {comma,shift+comma,period,shift+period}
	brightnessctl {set 5%-,set 1%, set +5%, set 100%}

super + {F1, shift + F1}
    {kdeconnect-indicator ; kdeconnect-cli --refresh, pkill kdeconnect}

super + {F2, shift + F2}
    redshift {-O 5000, -x}

super + {F3, shift + F3}
    {torwrap, td-toggle}

super + F4
    remaps && notify-send -t 1000 'Keyboard layout' 'Has been reset to defaults'

super + shift + F4
    remaps && xinput set-prop "ETPS/2 Elantech Touchpad" "Device Enabled" 0 & notify-send -t 1000 'Touchpad Has been disabled'

super + {F10, shift + F10, F11, F12, shift + F12}
    mpc {seek -1%, prev,  toggle, seek +1%, next}

super + shift + F11
    pauseallmpv

super + q
	physlock -s

super + shift + q
	sysact

super + ctrl + x
    xkill

super + shift + r
	pkill -USR1 -x sxhkd
