super + Return
	 fol -e Tmux

super + shift + Return
    fol -d -g -N Emacs -e "emacsclient -c"

super + alt + Return
	 $TERMINAL

super + alt + Return
    $TERMINAL

super + y
    fol -g -N jetbrains-idea-ce -e idea

super + shift + y
	fol -e "mycli -u root"

super + r
	$BROWSER $HOME/personal/readlater

super + o; h
	fol -e htop

super + o; {m,u}
	{dmenumount, dmenuumount}

super + w
	fol -g -e $BROWSER

super + shift + w
	fol -N Chromium -g -e chromium

super + {_,shift+} b
	{dmenu-bookmarks,bookmarkit}

super + {_,shift+} s
    {dmenu-snippets,snipit}

super + q
	dmenu_web

super + shift + p
    displayselect

#super + shift + p
#    passmenu


super + {_,shift+} d
	{dmenu_run , dmenuunicode}

super + v
	clipmenu -i

super + shift + v
	fol -e pulsemixer

super + t; {_,super+} t
	transs {C,P}

super + t; {a, e, p}
	fol -e "{transs ar,transs en,transs pr}"

super + t; {s,S}
	{fol -e sdcv,$TERMINAL -e sdcv $(xclip -o)}

super + a
    fol -g -N Anki -e anki

super + e
	fol -e lfub

super + shift + e
	fol -e bicon -e neomutt

super + n
	fol -e bicon -e newsboat


super + shift + n
	fol -e bicon -e ncmpcpp

{_,shift+} Print; {f, s, w}
	mymaim { ,-c} {-f,-s,-w}

Print; S
	flameshot gui

Print; r
	dmenurecord kill || dmenurecord

XF86Audio{Mute,LowerVolume,RaiseVolume}
	volume {-t,-d 5,-i 5}

super + {shift+minus,minus,equal}
	volume {-t,-d 5,-i 5}

{_,super+ } XF86MonBrightness{Down,Up}
	brightnessctl set {10%-, 1%, +10%, 100%}

super + {_,shift+ }{comma,period}
	brightnessctl {set 5%-,set 1%, set +5%, set 100%}

super + o; {_, shift+ } k
	{kdeconnect-indicator;! pidof kdeconnectd  && /usr/lib/kdeconnectd; kdeconnect-cli --refresh, killall kdeconnect-indicator; sleep 1&& killall kdeconnectd }

super + o; {_, shift+ } r
	redshift {-O 5000,-x}

super + o; {_, shift+ } t
	{fol -e torwrap, td-toggle}

super + {_, shift+ }Insert
	remaps; touchpad_toggle {off,on}

super + alt + Insert
	setxkbmap -model pc105 -layout us -option '' && notify-send -t 1000 "Setxkbmap" "Has been reset"

super + {_, shift+ }{bracketleft,bracketright}
	mpc {seek -1%, prev,  seek +1%, next}

super + backslash
	mpc repeat&& mpc single && notify-send -u low -t 1000 "MPD" "$(mpc status 'Single Repeat: %repeat%')"

super + {_, shift+ } p
	{mpc toggle, pauseallmpv}

super + i
	fol -e nmtui

super + shift + i
	sudo nmcli wifi rescan

# less used keybinds
super + o; {w,i,c,n,v,r}
	{forecast,$TERMINAL -e sh -c 'vnstat| less',fol -N bc -e "bc -lq",newword, mpv --ytdl-format=19 "$(xclip -sel c -o)" || mpv --ytdl-format=22 "$(xclip -sel c -o)", $BROWSER "https://roadmap.sh/cyber-security" }

super + o; e
	myenglish

super + g
	nsxiv -N pdfgallery -t ~/.cache/pdfgallery/*.jpg

super + {_,shift+,alt} BackSpace
	dunstctl {close-all,history-pop,set-paused toggle && notify-send "Dunst" "Is-paused: $(dunstctl is-paused)" }

#super + shift + {Right,Left}
	#dpselector {1,2}

super + o; C
	colorpicker

super + ctrl + x
	xkill

super + shift + q
	sysact

super + z
	physlock -s

super + shift + z
	loginctl suspend

super + Tab
    fol -a

# For some reason sxhkd hangs when opening vim and then pressing super+shift+r
# So keep it s-S-r instead of s-C-r
super + shift + r
	pkill -USR1 -x sxhkd; openbox --reconfigure
