super + Return
    $TERMINAL

super + Return
    $TERMINAL

super + shift + Return
    emacsclient  -c -a 'emacs'

super + p
    dmenu_run

super + shift + p
    passmenu

super + {grave, Insert, shift + Insert, b}
    {dmenuunicode -i, dmenu-bookmarks, bookmarkit, dmenu-snippets}

super + v
    clipmenu

super + shift + v
    $TERMINAL -e pulsemixer

super + {shift + slash, alt + slash}
    {transXclip, transXcclip}

super + w
    $BROWSER

super + shift + w
    weather-report show

super + e
    $TERMINAL -e lfub

super + {shift + e,  n, shift + r}
    qterminal -e {neomutt, ncmpcpp, sh -c 'killall newsboat ; newsboat'}

{Print, shift + Print, super + Print, super + Delete}
    {maim "$XDG_PICTURES_DIR/screenshots/$(date '+%y%m%d-%H%M-%S').png" pic-full- && notify-send -t 500 'ðŸŒ„ Screenshot has been printed', maimpick, dmenurecord -i, dmenurecord kill}

XF86Audio{RaiseVolume,LowerVolume}

# wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%{-,+} ; kill -44 $(pidof dwmblocks)

{XF86AudioMute, super+shift+minus}
    pamixer -t; kill -44 $(pidof dwmblocks)

super + { minus, equal}
    pamixer --allow-boost {-d , -i } 5; kill -44 $(pidof dwmblocks)

super + {comma,shift+comma,period,shift+period}
	brightnessctl {set 5%-,set 1%, set +5%, set 100%}

{XF86MonBrightnessDown, super + XF86MonBrightnessDown, XF86MonBrightnessUp, super + XF86MonBrightnessUp}
	brightnessctl set {10%-, 1%, +10%, 100%}

super + ctrl + x
    xkill

super + {shift + BackSpace, ctrl + BackSpace}
    dunstctl {history-pop, set-paused toggle && notify-send "Toggled dunst"}

super + {F10, shift + F10, F11, F12, shift + F12}
    mpc {prev, seek -1%, toggle, next, seek +1%}

super + shift + F11
    pauseallmpv

super + {F1, shift + F1}
    {kdeconnect-indicator ; kdeconnect-cli --refresh, kill $(pgrep kdeconnect)}

super + F2
    redshift -O 5000

super + shift + F2
    redshift -x

super + {F3, shift + F3}
    {torwrap, td-toggle}

super + {F4, shift + F4}
    {displayselect, dmenumount -i}

super + F5
    remaps && notify-send -t 1000 'Keyboard layout' 'Has been reset to defaults'

super + shift + F5
    remaps && xinput set-prop "ETPS/2 Elantech Touchpad" "Device Enabled" 0 & notify-send -t 1000 'Touchpad Has been disabled'

super + shift + q
	sysact

super + ctrl + r
	pkill -USR1 -x sxhkd
