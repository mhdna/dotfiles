super + Return
    $TERMINAL

super + i
    xdo activate -N Tmux || $TERMINAL -g 120x40 -c Tmux -e sh -c 'tmux a || tmux'

super + shift + i
    xdo activate -dN Emacs || emacsclient -c

super + y
    fol mycli mycli -u root

super + o; a
    xdo activate -N agenda || $TERMINAL -c agenda -g 40x20 -e emacsclient -nw -e '(org-agenda-list)' -e '(delete-other-windows)'

super + o; l
    emacsclient -c -e '(leetcode)'

super + o; h
    fol htop

super +  w; {o,O,s,S}
    {dmenu-bookmarks,dmenu-snippets,bookmarkit,snipit}

super + shift + w
    fol g $BROWSER

super + {_,shift+} {d,p}
    {dmenu_run, dmenuunicode -i, passmenu, displayselect}


super + v
    clipmenu -i

super + shift + v
    fol pulsemixer

super + t; {super + t, t}
    transs {P,C}

super + t; {a, e, p}
    fol {Transa transs a,Transe transs e,Transf transs p}

super + t; {s,S}
    {fol sdcv,$TERMINAL -e sdcv $(xclip -o)}

super + f
    fol lfub

super + BackSpace
    emacsclient -c -e '(my/set-arabic-font)'-e '(my/agenda)'

super + e
    fol neomutt bicon -e neomutt

super + n
    fol newsboat bicon -e newsboat

super + shift + n
    fol ncmpcpp

{_,shift+} Print; {a, s, w}
    mymaim { ,c}{a,s,w}

Print; S
    flameshot gui

Print; r
    kill -15 "$(cat /tmp/recordingpid)" && rm -f /tmp/recordingpid || dmenurecord -i

XF86Audio{LowerVolume,RaiseVolume}
    wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%{-,+}

{XF86AudioMute, super+shift+minus}
    wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle

super + { minus, equal}
    wpctl set-volume @DEFAULT_AUDIO_SINK@ {5%-, 5%+}

{_,super+ } XF86MonBrightness{Down,Up}
	brightnessctl set {10%-, 1%, +10%, 100%}

super + {_,shift+ }{comma,period}
	brightnessctl {set 5%-,set 1%, set +5%, set 100%}

super + {_, shift+ }F1
    {kdeconnect-indicator ; kdeconnect-cli --refresh, pkill kdeconnect}

super + {_, shift+ }F2
    redshift {-O 5000,-x}

super + {_, shift+ }F3
    {fol torwrap, td-toggle}

super + {_, shift+ }F4
    remaps; touchpad_toggle {off,on}

super + {_, shift+ }{F10,F12}
    mpc {seek -1%, prev,  seek +1%, next}

super + {_, shift+ }F11
    {mpc toggle, pauseallmpv}

# less used keybinds
super + o; {f,w,N,c,n,v}
    {fol flash,weather-report, $TERMINAL -e nmtui,xdo activate -N bc || $TERMINAL -g 30x20 -c bc -e bc -lq,newword, mpv --ytdl-format=22 "$(xclip -sel c -o)" }

super + o; { m, u}
    {dmenumount, dmenuumount}

super + ctrl + x
    xkill

super + shift + q
	sysact

super + z
    physlock -s

super + shift + z
	loginctl suspend

super + Home
    $TERMINAL -e soynet open

super + End
    soynet close

super + shift + r
	pkill -USR1 -x sxhkd&& notify-send -u low 'Sxhkd' 'Reloaded'
