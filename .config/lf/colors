# vim:ft=dircolors

# file types (with matching order)
ln      01;36   # LINK
or      37;41   # ORPHAN
tw      34      # STICKY_OTHER_WRITABLE
ow      34      # OTHER_WRITABLE
st      01;34   # STICKY
di      01;34   # DIR
pi      30;43      # FIFO
so      01;35   # SOCK
bd      30;43   # BLK
cd      30;43   # CHR
su      01;32   # SETUID
sg      01;32   # SETGID
ex      01;32   # EXEC
fi      00      # FILE

